<!DOCTYPE html>
<html lang="en">

<head>
    <link rel = "icon" href = "/images/sp.png" type = "image/x-icon">
    <title><%= course.name %></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <style>
    .jumbotron{
        background-color:rgba(0, 0, 0, 0.5);
    }
    .alert B{
        color:white;
    }
    .alert{
        background-color: rgba(0, 0, 0, 0.5);
    }
    .sidebar a{
            color:darkorange;
        }
     .sidebar a:hover{
            color: white;
             transform: scale(1.5);
        }
        .side-bar .responsive:hover {
            transform: scale(1.5);
        }
        a:hover{
            text-decoration: none;
        }
       
        .alert:hover{
            box-shadow:  3px 3px 5px 6px rgba(0, 0, 0, 0.5);
            font-size: 20px;
            color: black;
            background-color:darkslategrey;
            
            
        }
       
       
        .responsive:hover{
            box-shadow:  3px 3px 5px 6px rgba(0, 0, 0, 0.5);
        }
        .responsive {
  
  max-width: 300px;
  height: auto;
}
        /* width */
::-webkit-scrollbar {
  width: 5px;
}

/* Track */
::-webkit-scrollbar-track {
     background-color: rgba(0, 0, 0, 0.5);
  box-shadow: inset 0 0 0px grey; 
  border-radius: 0px;
}
 
/* Handle */
::-webkit-scrollbar-thumb {
  background: black; 
  border-radius: 10px;
}

/* Handle on hover */
::-webkit-scrollbar-thumb:hover {
  background: #b30000; 
}
       

        body {
            background-image: url('http://getwallpapers.com/wallpaper/full/a/5/d/544750.jpg');
            background-size: cover;
            background-repeat: no-repeat;
            background-attachment: fixed;
            height: 100%;
            font-family: 'Numans', sans-serif;
        }

        /* On small screens, set height to 'auto' for the grid */
        @media screen and (max-width: 767px) {
            .row.content {
                height: auto;
            }
        }

        div.content {
            margin-left: 330px;
        }

        .sidebar {
            
  
  top: 0;
  bottom: 0;
  left: 0;

  
  overflow-y: auto;

            margin-left: 0px;
            background-color: white;
            position: fixed;
        }

        /* On screens that are less than 700px wide, make the sidebar into a topbar */
        @media screen and (max-width: 700px) {
            .sidebar {
                width: 100%;
                height: auto;
                position: relative;
            }

            .sidebar a {
                float: left;
            }

            div.content {
                margin-left: 0;
            }
            .responsive{
                max-width: 100px;
            }
           
           
        }

        /* On screens that are less than 400px, display the bar vertically, instead of horizontally */
        @media screen and (max-width: 400px) {
            .sidebar a {
                text-align: center;
                float: none;
            }
            .navbar{
                position: fixed;
            }
            
        }
    </style>
</head>

<body>



    <div class="">

        <div class="col-sm-3 sidebar" style=" background-color: rgba(0, 0, 0, 0.5);margin-right:10%">
            <div class="container-fluid"><br>

                <a href="/courses/<%=course._id%>/references">
                    <img class="card-img-top responsive" src="/images/references.jpg" alt="Card image" style="width:300px; padding-top: 10px;padding-right: 10px">
                    <center>
                        <p class="pr" style="font-size: 20px">References</p>
                    </center>
                </a>
                <a href="/courses">

                    <!--<img class="card-img-top responsive" src=""Card image" style="width:300px; padding-top: 10px;padding-right: 10px">
                    -->
                    <img class="card-img-top responsive" src="/images/courses.png" alt="Card image" style="width:300px; padding-top: 10px;padding-right: 10px">
                    <center>
                        <p class="pr" style="font-size: 20px">Courses</p>
                    </center>
                </a>
                <a href="/forums">
                    <!--<img class="card-img-top responsive" src="data:image/jpeg;base64," alt="Card image" style="width:300px; padding-top: 10px;padding-right: 10px">
                    -->
                    <img class="card-img-top responsive" src="/images/forums.jpg" alt="Card image" style="width:300px; padding-top: 10px;padding-right: 10px">
                    <center>
                        <p class="pr" style="font-size: 20px">Forums</p>
                    </center>
                </a>

            </div>
        </div>
        <br>

        <div class="col-sm-9 content" style="padding-left:10% background-color: rgba(0, 0, 0, 0.5)">
            <nav class="navbar navbar-inverse">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/">Student_Partner</a>
                    </div>
                    <div class="collapse navbar-collapse" id="myNavbar">
                        <ul class="nav navbar-nav navbar-right">
                            <li><a href="/courses"><span class="glyphicon glyphicon-user"></span> Signed In As <%= currentUser.username %></a></li>
                            <li><a href="/logout"><span class="glyphicon glyphicon-log-in"></span> Logout</a></li>
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="jumbotron" style="text-align:center">
                <h1 style="color: white"><%= course.name %></h1>
                <h4 style="color: white"><%= course.description %></h4>
                <h4 class="pull-right"><span class="label label-primary">Instructor:</span> <span class="label label-default"><%=course.instructor.username %></span></h4>
                <% if(currentUser.username == course.instructor.username){ %>
                    <a href="/courses/<%= course._id %>/lessons/new"><button class="btn btn-warning btn-lg"><span class="btn-label"><i class="glyphicon glyphicon-plus-sign"></i></span>Add New Lesson</button></a>
                <% } %>
            </div>
            <% course.lessons.forEach(function(lesson){ %>
            <a  href="/courses/<%= course._id %>/<%= lesson._id %>">
                    <div class="alert " role="alert" >
                        <B class="bln">Unit-<%=lesson.lesson%><br> <%= lesson.name.charAt(0).toUpperCase() + lesson.name.slice(1) %></B>:
                        <B class="bln"><%= lesson.description %></B>
                        
                    </div>
                    </a>
            <% }); %>
            
